{
  "name": "swagclan",
  "version": "1.0.0",
  "main": "index.js",
  "license": "AGPL-3.0-only",
  "scripts": {
    "fix": "eslint ./src/**/*.ts --fix",
    "lint": "eslint ./src/**/*.ts",
    "test": "ts-mocha -p . src/**/*.spec.ts",
    "postinstall": "husky install",
    "build": "tsc -p ./",
    "watch": "tsc -w -p ./",
    "dev:api": "ts-node ./src/api/index.ts",
    "dev:client": "ts-node ./src/spawn/index.ts",
    "dev": "concurrently npm:dev:api npm:dev:client",
    "start:api": "node ./dist/api/index.js",
    "start:client": "node ./dist/spawn/index.js",
    "start": "concurrently npm:start:api npm:start:client"
  },
  "dependencies": {
    "@wilsonjs/client": "^0.0.19",
    "@wilsonjs/constants": "^0.0.19",
    "@wilsonjs/models": "^0.0.19",
    "chalk": "^4.1.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dayjs": "^1.10.4",
    "dotenv": "^8.2.0",
    "emittery": "^0.8.1",
    "express": "^4.17.1",
    "influx": "^5.8.0",
    "mongodb": "^3.6.5",
    "mongoose": "^5.12.10",
    "node-fetch": "^2.6.1",
    "querystring": "^0.2.1",
    "reflect-metadata": "^0.1.13",
    "src": "link:./src",
    "tedis": "^0.1.12",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "zod": "^1.11.11"
  },
  "devDependencies": {
    "@swc/core": "^1.2.57",
    "@swc/helpers": "^0.2.11",
    "@typegoose/typegoose": "^7.6.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/expect": "^24.3.0",
    "@types/express": "^4.17.11",
    "@types/mocha": "^8.2.1",
    "@types/node-fetch": "^2.5.8",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "concurrently": "^6.0.0",
    "eslint": "^7.21.0",
    "husky": "^5.2.0",
    "lint-staged": "^10.5.4",
    "mocha": "^8.3.1",
    "ts-mocha": "^8.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "dependenciesMeta": {
    "eslint@7.21.0": {
      "unplugged": true
    }
  },
  "workspaces": [
    "shared",
    "web"
  ]
}
